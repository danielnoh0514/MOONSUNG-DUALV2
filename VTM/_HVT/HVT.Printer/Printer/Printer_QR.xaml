<UserControl x:Class="HVT.Printer.Printer_QR"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:xc="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:local="clr-namespace:HVT.Printer"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="1000">
    <Grid>
        <Border Background="#222425" Margin="0" CornerRadius="10">
            <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width ="1*"/>
                <ColumnDefinition Width ="1*"/>
            </Grid.ColumnDefinitions>
            <Grid >
                <Grid.RowDefinitions>
                    <RowDefinition  Height="1*"/>
                    <RowDefinition  Height="Auto"/>
                    <RowDefinition  Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="2" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Top" Grid.Column="0" Margin="5" Background="{x:Null}">
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width ="1*"/>
                        <ColumnDefinition Width ="0.8*"/>
                        <ColumnDefinition Width ="1*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Grid.Row="0" BorderThickness="1,1,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"  HorizontalContentAlignment="Center">Item</Label>
                    <Label Grid.Column="0" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Unit code</Label>
                    <Label Grid.Column="0" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Vendor code</Label>
                    <Label Grid.Column="0" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Gubun code</Label>
                    <Label Grid.Column="0" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Country code</Label>
                    <Label Grid.Column="0" FontSize="15"   Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="Line code"/>
                    <Label Grid.Column="0" FontSize="15"   Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="EQP/N"/>


                    <Label Grid.Column="1" Grid.Row="0" BorderThickness="1,1,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"    HorizontalContentAlignment="Center">Digit</Label>
                    <Label Grid.Column="1" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                    <Label Grid.Column="1" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">4</Label>
                    <Label Grid.Column="1" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                    <Label Grid.Column="1" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                    <Label Grid.Column="1" FontSize="15"   Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="3"/>
                    <Label Grid.Column="1" FontSize="15"   Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="4"/>

                    <Label Grid.Column="2" FontSize="15"   Grid.Row="0" BorderThickness="1,1,1,1" BorderBrush="#898F98" Foreground="White"  HorizontalContentAlignment="Center">Item</Label>
                    <TextBox x:Name="tbQR_UnitCode" FontSize="15"   Grid.Column="2" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">06</TextBox>
                    <TextBox x:Name="tbQR_VenderCode" FontSize="15"   Grid.Column="2" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">DYSC</TextBox>
                    <TextBox x:Name="tbQR_GubunCode" FontSize="15"   Grid.Column="2" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">QR</TextBox>
                    <TextBox x:Name="tbQR_CountryCode" FontSize="15"   Grid.Column="2" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">VN</TextBox>
                    <TextBox x:Name="tbQR_LineCode" FontSize="15"   Grid.Column="2" Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Text="L01"/>
                    <TextBox x:Name="tbQR_EQPMCode" FontSize="15"   Grid.Column="2" Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Text="EQP1"/>

                </Grid>
                <Canvas x:Name="CanvasRinbol" VerticalAlignment="Center" HorizontalAlignment="Center" Background="Gray"
                        Height="{Binding Path=DEV_Label.RHeight}" Width="{Binding  Path=DEV_Label.RWidth}">

                    <Canvas x:Name="labelCanvasViewer" Background="White" HorizontalAlignment="Center" VerticalAlignment="Center"
                        Height="{Binding Path=DEV_Label.Height}" Width="{Binding  Path=DEV_Label.Width}"
                        Canvas.Top="{Binding Path=DEV_Label.HomeY}" Canvas.Left="{Binding Path=DEV_Label.HomeX}">

                        <Image x:Name="imgQr" Source="{Binding Path=DEV_Label.QR.Content}" Width="{Binding Path=DEV_Label.QR.Width}" Height="{Binding Path=DEV_Label.QR.Height}" Stretch="Fill" Canvas.Top="{Binding Path=DEV_Label.QR.Top}" Canvas.Left="{Binding Path=DEV_Label.QR.Left}"/>
                        <Label x:Name="lbSN1"  FontWeight="Bold"  Content="06DA9200718A" FontSize="{Binding Path=DEV_Label.SN1.Size}" Padding="0" Canvas.Top="{Binding Path=DEV_Label.SN1.Top}" Canvas.Left="{Binding Path=DEV_Label.SN1.Left}" Visibility="{Binding DEV_Label.SN1.Visibility}"/>
                        <Label x:Name="lbMainVer"  FontWeight="Bold"  Content="01507A-150920" FontSize="{Binding Path=DEV_Label.detail1.Size}" Padding="0" Canvas.Top="{Binding Path=DEV_Label.detail1.Top}" Canvas.Left="{Binding Path=DEV_Label.detail1.Left}" Visibility="{Binding DEV_Label.detail1.Visibility}"/>
                        <Label x:Name="lbSN2"   FontWeight="Bold"  Content="06DA9200718A" FontSize="{Binding Path=DEV_Label.SN2.Size}" Padding="0" Canvas.Top="{Binding Path=DEV_Label.SN2.Top}" Canvas.Left="{Binding Path=DEV_Label.SN2.Left}"  Visibility="{Binding DEV_Label.SN2.Visibility}">
                            <Label.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Label.RenderTransform>
                        </Label>

                        <Label x:Name="lbInvVer" FontWeight="Bold"  Content="01507A-150920" FontSize="{Binding Path=DEV_Label.detail2.Size}" Padding="0" Canvas.Top="{Binding Path=DEV_Label.detail2.Top}" Canvas.Left="{Binding Path=DEV_Label.detail2.Left}"  Visibility="{Binding DEV_Label.detail2.Visibility}">
                            <Label.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Label.RenderTransform>
                        </Label>
                        <Label x:Name="lbSiteName"  FontWeight="Bold"  Content="A" Background="Black" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Width="{Binding Path=DEV_Label.ArrayText.Width}" Height="{Binding Path=DEV_Label.ArrayText.Height}"
                                                   Foreground="White" Padding="0" FontSize="{Binding Path=DEV_Label.ArrayText.Size}" Canvas.Top="{Binding Path=DEV_Label.ArrayText.Top}" Canvas.Left="{Binding Path=DEV_Label.ArrayText.Left}"  Visibility="{Binding DEV_Label.ArrayText.Visibility}"/>
                    </Canvas>

                </Canvas>
                <TextBox Margin="5" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Text="{Binding Path=DEV_Label.QR.ContentString, UpdateSourceTrigger=PropertyChanged}" Grid.Row="1" Height="30"/>
            </Grid>
            <StackPanel Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0" >
                <Grid Margin="10,0,10,0" Width="500">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="133*"/>
                        <ColumnDefinition Width="131*"/>
                        <ColumnDefinition Width="107*"/>
                        <ColumnDefinition Width="71*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0"  FontSize="12" FontWeight="Bold"  Content="QR print condition" Foreground="LightGray"  />
                    <Label Grid.Row="1"  FontSize="12" FontWeight="Bold"  Content="Print max step count" Foreground="LightGray"  />
                    <Label Grid.Row="2"  FontSize="12" FontWeight="Bold"  Content="Product line" Foreground="LightGray"  Visibility="Hidden" />

                    <RadioButton x:Name="rbtTestPassAll"  Grid.Row="0" Grid.Column="1" Margin="5"  FontSize="12" FontWeight="Bold"  Content="Test PCB pass all" Foreground="LightGray" VerticalAlignment="Center"/>
                    <RadioButton x:Name="rbtPassOnly" Grid.Row="0" Grid.Column="2" Margin="5"  FontSize="12" FontWeight="Bold"  Content="Pass PCB only" Foreground="LightGray" VerticalAlignment="Center"/>

                    <xc:IntegerUpDown x:Name="nudPrintMaxStepCount" Grid.Column="1" Grid.Row="1" Margin="0" VerticalAlignment="Center" Maximum="100000" Minimum="0" Value="3" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True"/>
                    <Label Grid.Row="3" Grid.Column="0"  FontSize="12" FontWeight="Bold"  Content="Product Serial base" Foreground="LightGray"  Visibility="Visible"/>
                    <xc:IntegerUpDown x:Name="nudSerialBase" Grid.Column="1" Grid.Row="3" Margin="0" VerticalAlignment="Center" Maximum="9999" Minimum="0" Value="1" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True"/>

                </Grid>
                <StackPanel Margin="10,10,10,0">
                    <Grid Margin="0,0,0,20" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="119*"/>
                            <ColumnDefinition Width="122*"/>
                            <ColumnDefinition Width="121*"/>
                            <ColumnDefinition Width="121*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Button x:Name="btDefault"  FontSize="12" FontWeight="Bold"  Content="Default" Grid.Column="3" Grid.Row="1" Height="25" Margin="2,0,0,0" Click="btDefault_Click" />
                        <Button x:Name="btUndoall"   FontSize="12" FontWeight="Bold"  Content="Undo all" Grid.Column="2" Grid.Row="1" Height="25" Margin="2,0,0,0" />


                        <Label Grid.Row="2"  FontSize="12" FontWeight="Bold"  Content="Print speed" Foreground="LightGray" Margin="0,0,0,0" HorizontalAlignment="Right"/>
                        <Label Grid.Row="3"  FontSize="12" FontWeight="Bold"  Content="Darkness" Foreground="LightGray" Margin="0,0,0,0" HorizontalAlignment="Right"/>
                        <Label Grid.Row="3" Grid.Column="2"  FontSize="12" FontWeight="Bold"  Content="Label pad" Foreground="LightGray" Margin="0" HorizontalAlignment="Right"/>

                        <xc:IntegerUpDown x:Name="nudPrintSpeed" Grid.Column="1" Grid.Row="2" Margin="14,0,5,0" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path = DEV_Label.Speed}"/>
                        <xc:IntegerUpDown x:Name="nudDarkness" Grid.Column="1" Grid.Row="3" Margin="14,0,5,0" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path = DEV_Label.Darkness}"/>
                        <xc:IntegerUpDown x:Name="nudLabelPad" Grid.Column="3" Grid.Row="3" Margin="14,0,5,0" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path = DEV_Label.Pad}"/>

                        <Label Grid.Row="4" Grid.Column="0"  FontSize="12" FontWeight="Bold"  Content="Label Width" Foreground="LightGray" Margin="0" HorizontalAlignment="Right"/>
                        <Label Grid.Row="4" Grid.Column="2"  FontSize="12" FontWeight="Bold"  Content="Label Height" Foreground="LightGray" Margin="0" HorizontalAlignment="Right"/>

                        <xc:IntegerUpDown x:Name="nudLabelWidth" Grid.Column="1" Grid.Row="4" Margin="14,0,5,0" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path = DEV_Label.Width}"/>
                        <xc:IntegerUpDown x:Name="nudLabelHeight" Grid.Column="3" Grid.Row="4" Margin="14,0,5,0" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path = DEV_Label.Height}"/>

                    </Grid>
                    <Grid Margin="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="10"/>
                        </Grid.RowDefinitions>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="1. Label Home" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudLabelHomeX" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.HomeX}"/>
                            <xc:IntegerUpDown x:Name="nudLableHomeY" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.HomeY}"/>
                        </Grid>

                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="2. QR-Code" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudQRCodeX" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.QR.Top}"/>
                            <xc:IntegerUpDown x:Name="nudQRCodeY" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.QR.Left}"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="Size:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSize" Grid.Column="4" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.QR.Size}"/>
                        </Grid>
                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="3. S/N Part 1" Foreground="LightGray"/>
                            <CheckBox      Content="Print" VerticalContentAlignment="Center" Grid.Column="2" FontSize="10" FontWeight="Bold" Foreground="LightGray" IsChecked="{Binding Path=DEV_Label.SN1.Print}"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1X" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN1.Left}"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1Y" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN1.Top}"/>
                            <Label            Grid.Column="1" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="font:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="w:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="h:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbSNPart1Font"  FontSize="10"  Grid.Column="2" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Background="White" Text="{Binding Path=DEV_Label.SN1.Size, UpdateSourceTrigger=PropertyChanged}"/>

                            <xc:IntegerUpDown x:Name="nudSNPart1W" Grid.Column="4" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN1.Width}"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1H" Grid.Column="6" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN1.Height}"/>
                        </Grid>
                        <Grid Grid.Row="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="4. S/N Part 2" Foreground="LightGray"/>
                            <CheckBox      Content="Print" VerticalContentAlignment="Center" Grid.Column="2" FontSize="10" FontWeight="Bold" Foreground="LightGray" IsChecked="{Binding Path=DEV_Label.SN2.Print}"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2X" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN2.Left}"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2Y" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN2.Top}"/>
                            <Label            Grid.Column="1" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="font:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="w:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="h:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbSNPart2Font"  FontSize="10"  Grid.Column="2" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Background="White" Text="{Binding Path=DEV_Label.SN2.Size, UpdateSourceTrigger=PropertyChanged}"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2W" Grid.Column="4" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN2.Width}"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2H" Grid.Column="6" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="{Binding Path=DEV_Label.SN2.Height}"/>
                        </Grid>
                        <Grid Grid.Row="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
