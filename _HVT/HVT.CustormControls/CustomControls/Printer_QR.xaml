<UserControl x:Class="CustomControls.Printer_QR"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:xc="http://schemas.xceed.com/wpf/xaml/toolkit"
             xmlns:local="clr-namespace:CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="1000">
    <Grid Background="Black">
        <Grid Background="#222425" Margin="-2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width ="1*"/>
                <ColumnDefinition Width ="1*"/>
            </Grid.ColumnDefinitions>
            <Grid Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Top" Grid.Column="0" Margin="5" Background="{x:Null}">
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width ="1*"/>
                    <ColumnDefinition Width ="0.8*"/>
                    <ColumnDefinition Width ="1*"/>
                </Grid.ColumnDefinitions>
                <Label Grid.Column="0" Grid.Row="0" BorderThickness="1,1,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"  HorizontalContentAlignment="Center">Item</Label>
                <Label Grid.Column="0" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Unit code</Label>
                <Label Grid.Column="0" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Vendor code</Label>
                <Label Grid.Column="0" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Gubun code</Label>
                <Label Grid.Column="0" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">Country code</Label>
                <Label Grid.Column="0" FontSize="15"   Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="Line code"/>
                <Label Grid.Column="0" FontSize="15"   Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="EQP/N"/>


                <Label Grid.Column="1" Grid.Row="0" BorderThickness="1,1,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"    HorizontalContentAlignment="Center">Digit</Label>
                <Label Grid.Column="1" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                <Label Grid.Column="1" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">4</Label>
                <Label Grid.Column="1" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                <Label Grid.Column="1" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" FontSize="15"   Background="#43474D" HorizontalContentAlignment="Center">2</Label>
                <Label Grid.Column="1" FontSize="15"   Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="3"/>
                <Label Grid.Column="1" FontSize="15"   Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="White" Background="#43474D" HorizontalContentAlignment="Center" Content="4"/>

                <Label Grid.Column="2" FontSize="15"   Grid.Row="0" BorderThickness="1,1,1,1" BorderBrush="#898F98" Foreground="White"  HorizontalContentAlignment="Center">Item</Label>
                <TextBox x:Name="tbQR_UnitCode" FontSize="15"   Grid.Column="2" Grid.Row="1" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black" VerticalContentAlignment="Center" HorizontalContentAlignment="Center">06</TextBox>
                <TextBox x:Name="tbQR_VenderCode" FontSize="15"   Grid.Column="2" Grid.Row="2" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">DYSSC</TextBox>
                <TextBox x:Name="tbQR_GubunCode" FontSize="15"   Grid.Column="2" Grid.Row="3" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">QR</TextBox>
                <TextBox x:Name="tbQR_CountryCode" FontSize="15"   Grid.Column="2" Grid.Row="4" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center">VN</TextBox>
                <TextBox x:Name="tbQR_LineCode" FontSize="15"   Grid.Column="2" Grid.Row="6" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Text="L01"/>
                <TextBox x:Name="tbQR_EQPMCode" FontSize="15"   Grid.Column="2" Grid.Row="7" BorderThickness="1,0,0,1" BorderBrush="#898F98" Foreground="Black"  VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Text="EQP1"/>

            </Grid>

            <StackPanel Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0" >
                <Grid Margin="10,0,10,0" Width="500">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="133*"/>
                        <ColumnDefinition Width="131*"/>
                        <ColumnDefinition Width="107*"/>
                        <ColumnDefinition Width="71*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0"  FontSize="12" FontWeight="Bold"  Content="QR print condition" Foreground="LightGray"  />
                    <Label Grid.Row="1"  FontSize="12" FontWeight="Bold"  Content="Print max step count" Foreground="LightGray"  />
                    <Label Grid.Row="2"  FontSize="12" FontWeight="Bold"  Content="PCB array text print" Foreground="LightGray"  />
                    <Label Grid.Row="3"  FontSize="12" FontWeight="Bold"  Content="Product line" Foreground="LightGray"  Visibility="Hidden" />

                    <RadioButton x:Name="rbtTestPassAll" IsChecked="{Binding Path=Program.QR.TestPCBPrintAll}" Grid.Row="0" Grid.Column="1" Margin="5"  FontSize="12" FontWeight="Bold"  Content="Test PCB pass all" Foreground="LightGray" VerticalAlignment="Center"/>
                    <RadioButton x:Name="rbtPassOnly" Grid.Row="0" Grid.Column="2" Margin="5"  FontSize="12" FontWeight="Bold"  Content="Pass PCB only" Foreground="LightGray" VerticalAlignment="Center"/>

                    <xc:IntegerUpDown x:Name="nudPrintMaxStepCount" Grid.Column="1" Grid.Row="1" Margin="0" VerticalAlignment="Center" Maximum="100000" Minimum="0" Value="3" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True"/>
                    <CheckBox x:Name="cbPCBArrayPrint" Grid.Row="2" Grid.Column="1"  FontSize="12" FontWeight="Bold"  Content="Use" Margin="15,5,5,5" Foreground="LightGray"/>

                    <Label Grid.Row="2" Grid.Column="2"  FontSize="12" FontWeight="Bold"  Content="Country Code" Foreground="LightGray"   Visibility="Hidden"/>
                    <TextBox x:Name="tbCountryCode" Grid.Row="2" Grid.Column="3" Text="VN" Margin="0,4,10,4" FontSize="12"  Visibility="Hidden" />
                    <TextBox x:Name="tbProductLine"  Grid.Row="3" Grid.Column="1" Text="L01" Margin="5,4" FontSize="12"  Visibility="Hidden" />
                    <Label  Grid.Row="3" Grid.Column="2"  FontSize="12" FontWeight="Bold"  Content="Product Machine" Foreground="LightGray"  Visibility="Hidden" />
                    <TextBox x:Name="tbProductMachine" Grid.Row="3" Grid.Column="3" Text="T002" Margin="0,4,10,4" FontSize="12" Visibility="Hidden"/>

                    <Label Grid.Row="4" Grid.Column="0"  FontSize="12" FontWeight="Bold"  Content="Product Serial base" Foreground="LightGray"  Visibility="Visible"/>
                    <xc:IntegerUpDown x:Name="nudSerialBase" Grid.Column="1" Grid.Row="3" Margin="0" VerticalAlignment="Center" Maximum="9999" Minimum="0" Value="1" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True"/>

                </Grid>
                <StackPanel Margin="10,10,10,0">
                    <Grid Margin="0" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Canvas  Background="White" Grid.Row="1" Height="150" Width="150" Margin="75,5,5,5" Grid.RowSpan="5">
                            <Label  FontSize="12" FontWeight="Bold"  Content="1" Canvas.Top="-8" Canvas.Left="-8" Foreground="Red" Padding="0"/>
                            <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="0" Canvas.Left="0"/>

                            <Image x:Name="imgQr"  Width="109" Canvas.Top="10" Canvas.Left="10" Source="qr-code.png" Height="111"/>
                            
                            <Label  FontSize="12" FontWeight="Bold"  Content="2" Margin="-15,0,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="13" Canvas.Top="{Binding (Canvas.Top), ElementName=imgQr}" Canvas.Left="{Binding (Canvas.Left), ElementName=imgQr}" Foreground="Red" Padding="0"/>
                            <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=imgQr}" Canvas.Left="{Binding (Canvas.Left), ElementName=imgQr}"/>

                            <Label x:Name="lbSN1"  FontWeight="Bold"  Content="06DA9200718A" Canvas.Top="123" FontSize="15" Padding="0" Canvas.Left="2"/>
                            <Label  FontSize="12" FontWeight="Bold"  Content="3" HorizontalContentAlignment="Right" Width="7" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSN1}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSN1}" Foreground="Red" Padding="0" Margin="-10,0,0,0" />
                            <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSN1}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSN1}"/>

                            <!--<Label x:Name="lbMainVer"  FontWeight="Bold"  Content="01507A-150920" Canvas.Top="130" FontSize="11" Padding="0" Canvas.Left="7"/>
                                        <Label  FontSize="12" FontWeight="Bold"  Content="5" HorizontalContentAlignment="Right" Width="7" Canvas.Top="{Binding (Canvas.Top), ElementName=lbMainVer}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbMainVer}" Foreground="Red" Padding="-5,0,0,0" Margin="-10,0,0,0" />
                                        <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=lbMainVer}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbMainVer}"/>-->

                            <Label x:Name="lbSN2"   FontWeight="Bold"  Content="06DA9200718A" Canvas.Top="106" FontSize="14" Padding="0" Canvas.Left="127">
                                <Label.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform Angle="-90"/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Label.RenderTransform>
                            </Label>

                            <Label  FontSize="12" FontWeight="Bold"  Content="4" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="13" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSN2}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSN2}" Foreground="Red" Padding="0"/>
                            <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSN2}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSN2}"/>


                            <!--<Label x:Name="lbInvVer" FontWeight="Bold"  Content="01507A-150920" Canvas.Top="106" FontSize="11" Padding="0" Canvas.Left="131">
                                            <Label.RenderTransform>
                                                <TransformGroup>
                                                    <ScaleTransform/>
                                                    <SkewTransform/>
                                                    <RotateTransform Angle="-90"/>
                                                    <TranslateTransform/>
                                                </TransformGroup>
                                            </Label.RenderTransform>
                                        </Label>
                                        <Label  FontSize="12" FontWeight="Bold"  Content="6" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="13" Canvas.Top="{Binding (Canvas.Top), ElementName=lbInvVer}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbInvVer}" Foreground="Red" Padding="0"/>
                                        <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=lbInvVer}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbInvVer}"/>-->

                            <Label x:Name="lbSiteName"  FontWeight="Bold"  Content="A" Canvas.Top="123" FontSize="11" Background="Black" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"
                                                   Foreground="White" Padding="0" Canvas.Left="121" Width="22" Height="22"/>
                            <Label  FontSize="12" FontWeight="Bold"  Content="7" VerticalAlignment="Top" HorizontalContentAlignment="Right" Width="13" Margin="-15,0,0,0" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSiteName}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSiteName}" Foreground="Red" Padding="0"/>
                            <Ellipse Fill="Red" Width="5" Height="5" Canvas.Top="{Binding (Canvas.Top), ElementName=lbSiteName}" Canvas.Left="{Binding (Canvas.Left), ElementName=lbSiteName}"/>
                        </Canvas>

                        <Button x:Name="btDefault"  FontSize="12" FontWeight="Bold"  Content="Default" Grid.Column="2" Grid.Row="1" Height="25" Margin="2,0,5,0" Click="btDefault_Click" />
                        <Button x:Name="btUndoall"   FontSize="12" FontWeight="Bold"  Content="Undo all" Grid.Column="1" Grid.Row="1" Height="25" Margin="2,0,5,0" />


                        <Label Grid.Row="2" Grid.Column="1"  FontSize="12" FontWeight="Bold"  Content="Print speed" Foreground="LightGray"/>
                        <Label Grid.Row="3" Grid.Column="1"  FontSize="12" FontWeight="Bold"  Content="Darkness" Foreground="LightGray"/>
                        <Label Grid.Row="4" Grid.Column="1"  FontSize="12" FontWeight="Bold"  Content="Label lenght" Foreground="LightGray" />
                        <Label Grid.Row="5" Grid.Column="1"  FontSize="12" FontWeight="Bold"  Content="Label pad" Foreground="LightGray" />

                        <xc:IntegerUpDown x:Name="nudPrintSpeed" Grid.Column="2" Grid.Row="2" Margin="15,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        <xc:IntegerUpDown x:Name="nudDarkness" Grid.Column="2" Grid.Row="3" Margin="15,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        <xc:IntegerUpDown x:Name="nudLabelLenght" Grid.Column="2" Grid.Row="4" Margin="15,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        <xc:IntegerUpDown x:Name="nudLabelPad" Grid.Column="2" Grid.Row="5" Margin="15,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0"  FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="18"/>
                    </Grid>
                    <Grid Margin="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="10"/>
                        </Grid.RowDefinitions>

                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="1. Label Home" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudLabelHomeX" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <xc:IntegerUpDown x:Name="nudLableHomeY" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        </Grid>

                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="2. QR-Code" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudQRCodeX" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <xc:IntegerUpDown x:Name="nudQRCodeY" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="Size:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSize" Grid.Column="4" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        </Grid>
                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="3. S/N Part 1" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1X" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1Y" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <Label            Grid.Column="1" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="font:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="w:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="h:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbSNPart1Font"  FontSize="10"  Grid.Column="2" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Background="White" Text="3">

                            </TextBox>

                            <xc:IntegerUpDown x:Name="nudSNPart1W" Grid.Column="4" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <xc:IntegerUpDown x:Name="nudSNPart1H" Grid.Column="6" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                        </Grid>
                        <Grid Grid.Row="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Label            Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3"  FontSize="10" FontWeight="Bold"  Content="4. S/N Part 2" Foreground="LightGray"/>
                            <Label            Grid.Column="3" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="X:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="0"  FontSize="10" FontWeight="Bold"  Content="Y:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2X" Grid.Column="4" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <xc:IntegerUpDown x:Name="nudSNPart2Y" Grid.Column="6" Grid.Row="0" Margin="5,1,5,1" VerticalAlignment="Center" Maximum="100000" Minimum="0" FontSize = "12" Background="#222425" Foreground="White" BorderThickness="0,0,0,1" BorderBrush="White" ClipValueToMinMax="True" Value="5"/>
                            <Label            Grid.Column="1" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="font:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="3" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="w:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <Label            Grid.Column="5" Grid.Row="1"  FontSize="10" FontWeight="Bold"  Content="h:" Foreground="LightGray" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbSNPart2Font"  FontSize="10"  Grid.Column="2" Grid.Row="1" Margin="5,1,5,1" VerticalAlignment="Center" Background="White" Text="3"/>
